{
  "name": "leaders-debate",
  "version": "0.0.1",
  "description": "An Australian Federal Election Leaders Debate, reimagined by a neural network.",
  "main": "index.js",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 11"
  ],
  "watch": {
    "dev": "{www-src}/*"
  },
  "scripts": {
    "watch": "npm-watch",
    "generate": "python rnn/debate.py -t .2 -o rnn/generated/debate.html -p models/pm/full -b models/shorten/full",
    "copydebate": "cp rnn/generated/debate.html www-src/html-modules/content/debate.html",
    "clean": "rm -rf .cache dist",
    "dev": "parcel serve www-src/*.html",
    "build": "npm run clean && parcel build www-src/*.html -d dist/"
  },
  "keywords": [
    "neural-networks",
    "machine-learning",
    "politics"
  ],
  "author": "Advance Party <advanceparty@theroyals.com.au> (http://www.advanceparty.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "concurrently": "^4.1.0",
    "cssnano": "^4.1.10",
    "http-server": "^0.11.1",
    "natural": "^0.6.3",
    "npm-watch": "^0.6.0",
    "parcel": "^1.12.3",
    "postcss-assets": "^5.0.0",
    "posthtml": "^0.11.4",
    "posthtml-expressions": "^1.1.0",
    "posthtml-modules": "^0.4.2",
    "sass": "^1.20.1"
  }
}
