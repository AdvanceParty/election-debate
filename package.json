{
  "name": "leaders-debate",
  "version": "0.0.1",
  "description": "An Australian Federal Election Leaders Debate, reimagined by a neural network.",
  "main": "index.js",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "IE 11"
  ],
  "watch": {
    "build:dev": "{www-src}/*"
  },
  "scripts": {
    "watch": "npm-watch",
    "generate": "python rnn/debate.py -n 30 -t .8 -o rnn/generated/debate.html",
    "sample": "python rnn/debate.py -n 5 -t .8",
    "clean": "rm -rf dist",
    "build:dev": "parcel serve www-src/*.html",
    "build:prod": "parcel build www-src/*.html -d dist/",
    "prod": "npm run clean && npm run build:prod"
  },
  "keywords": [
    "neural-networks",
    "machine-learning",
    "politics"
  ],
  "author": "Gil Fewster <gil@theroyals.com.au> (http://www.advanceparty.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "cssnano": "^4.1.10",
    "npm-watch": "^0.6.0",
    "parcel": "^1.12.3",
    "postcss-assets": "^5.0.0",
    "posthtml": "^0.11.4",
    "posthtml-expressions": "^1.1.0",
    "posthtml-modules": "^0.4.2",
    "sass": "^1.20.1"
  }
}
